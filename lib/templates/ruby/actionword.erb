def <%= @rendered_children[:name].underscore %><% if has_parameters? %>(<%= @rendered_children[:parameters].join(', ') %>)<% end %>
<% unless @rendered_children[:tags].empty? %>  # Tags: <%= @rendered_children[:tags].join(' ') %>
<%= "\n" %><% end %>
<%= indent_block(@rendered_children[:body]) %>
<% if has_step? %>
<%= indent_block(["raise NotImplementedError"]) %>
<% end %>
end