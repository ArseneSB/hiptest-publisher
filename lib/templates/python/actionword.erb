def <%= @rendered_children[:name].underscore %>(<%= (['self'] << @rendered_children[:parameters]).flatten.join(', ') %>):
<% unless @rendered_children[:tags].empty? %>    # Tags: <%= @rendered_children[:tags].join(' ') %>
<%= "\n" %><% end %>
