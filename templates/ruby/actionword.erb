def <%= @rendered_childs[:name].underscore %>(<%= @rendered_childs[:parameters].join(', ') %>)
<% unless @rendered_childs[:tags].empty? %>  # Tags: <%= @rendered_childs[:tags].join(' ') %>
<%= "\n" %><% end %>
<%= indent_block(@rendered_childs[:body]) %>
<% if has_step? %>
<%= indent_block(["raise NotImplementedError"]) %>
<% end %>
end